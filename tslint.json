{
  "defaultSeverity": "error",
  "jsRules": {},
  "rulesDirectory": [],
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-var-requires": false,
    "max-line-length": [
      true,
      80
    ],
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declaration"
    ]
  }
}